{
  "name": "@ai-speech-evaluator/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:clean": "npm run kill-port && npm run dev",
    "kill-port": "lsof -ti:3002 | xargs kill -9 2>/dev/null || echo 'Port 3002 is available'",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ai-speech-evaluator/ai-services": "*",
    "@ai-speech-evaluator/audio-services": "*",
    "@ai-speech-evaluator/database": "*",
    "@ai-speech-evaluator/shared": "*",
    "@pinecone-database/pinecone": "^1.1.0",
    "bullmq": "^4.15.0",
    "compression": "^1.8.1",
    "cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.0",
    "joi": "^17.11.0",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-whisper": "^2024.11.13",
    "ollama": "^0.5.18",
    "openai": "^4.20.0",
    "pdf-parse": "^1.1.0",
    "rate-limiter-flexible": "^3.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.0.0"
  }
}
